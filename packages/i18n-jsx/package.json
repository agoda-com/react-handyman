{
  "name": "i18n-jsx",
  "description": "React (JS) text internationalization",
  "version": "0.3.0",
  "license": "MIT",
  "author": "Maciej Malkowski <monkey3310@gmail.com>",
  "repository": "matmalkowski/react-handyman",
  "main": "lib/i18n-jsx.cjs.js",
  "module": "lib/i18n-jsx.esm.js",
  "browser": "lib/i18n-jsx.umd.js",
  "types": "lib/index.d.ts",
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "dependencies": {
    "format-to-jsx": "^0.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "@types/react": "16.8.19",
    "nodecat": "^2.0.0",
    "react": "^16.8.6",
    "rimraf": "^2.6.3",
    "rollup": "^1.14.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-terser": "^5.0.0",
    "typescript": "3.5.1"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "rollup -c && yarn emit-declarations",
    "watch": "rollup -c -w",
    "typecheck": "tsc",
    "emit-declarations": "tsc -d --noEmit false --emitDeclarationOnly --isolatedModules false --declarationDir \"./lib/\""
  }
}
